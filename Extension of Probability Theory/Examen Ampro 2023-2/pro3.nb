Cell["Calculemos el modelo de probabilidad asociado a Yn", "Text",
 CellChangeTimes->{{3.9113598341557455`*^9, 
  3.9113598435536456`*^9}},ExpressionUUID->"90f41120-6767-4072-aa51-\
fbe898851190"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Yn", "[", 
   RowBox[{"n_", ",", "\[Alpha]_"}], "]"}], " ", ":=", " ", 
  RowBox[{"TransformedDistribution", "[", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"Xn", "-", 
        RowBox[{"n", "*", "\[Alpha]"}]}], ")"}], "/", "n"}], ")"}], ",", " ", 
    RowBox[{"Xn", "\[Distributed]", 
     RowBox[{"GammaDistribution", "[", 
      RowBox[{
       RowBox[{"n", "/", "\[Alpha]"}], ",", " ", 
       RowBox[{"2", "\[Alpha]"}]}], "]"}]}], ",", 
    RowBox[{"Assumptions", "->", 
     RowBox[{"\[Alpha]", ">", "0"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.9113585673363104`*^9, 3.9113586386579447`*^9}, 
   3.9113586782696238`*^9, {3.911360404289974*^9, 3.911360414263847*^9}},
