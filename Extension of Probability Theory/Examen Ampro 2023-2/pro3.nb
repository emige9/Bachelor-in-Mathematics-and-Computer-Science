 RowBox[{
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{"-", "\[ImaginaryI]"}], " ", "t", " ", "\[Alpha]"}]], " ", 
  SuperscriptBox[
   RowBox[{"(", 
    RowBox[{"1", "-", 
     FractionBox[
      RowBox[{"2", " ", "\[ImaginaryI]", " ", "t", " ", "\[Alpha]"}], "n"]}], 
    ")"}], 
   RowBox[{"-", 
    FractionBox["n", "\[Alpha]"]}]]}]], "Output",
 CellChangeTimes->{3.9113603722303677`*^9, 3.9113604193623843`*^9},
